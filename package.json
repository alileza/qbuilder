{
  "name": "qbuilder",
  "version": "0.1.2",
  "description": "Modular questionnaire engine with branching logic, validation, and versioning",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "src/db/migrations/*.sql",
    "README.md",
    "LICENSE",
    "CHANGELOG.md"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsup",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "lint": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "questionnaire",
    "survey",
    "form",
    "validation",
    "zod",
    "branching",
    "branching-logic",
    "conditional-logic",
    "dynamic-forms",
    "typescript",
    "postgres",
    "express",
    "rest-api",
    "openapi"
  ],
  "author": "Ali Reza Yahya <@alileza>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/alileza/qbuilder.git"
  },
  "bugs": {
    "url": "https://github.com/alileza/qbuilder/issues"
  },
  "homepage": "https://github.com/alileza/qbuilder#readme",
  "engines": {
    "node": ">=20.0.0"
  },
  "dependencies": {
    "zod": "^4.1.13"
  },
  "peerDependencies": {
    "express": "^4.0.0 || ^5.0.0",
    "pg": "^8.0.0"
  },
  "peerDependenciesMeta": {
    "express": {
      "optional": true
    }
  },
  "devDependencies": {
    "@types/express": "^5.0.5",
    "@types/node": "^24.10.1",
    "@types/pg": "^8.15.6",
    "@vitest/coverage-v8": "^4.0.14",
    "express": "^5.1.0",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3",
    "vitest": "^4.0.14"
  }
}
