{
  "name": "qbuilder-example",
  "version": "1.0.0",
  "description": "Example application using qbuilder",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "tsc && node dist/index.js",
    "db:up": "docker-compose up -d",
    "db:down": "docker-compose down",
    "db:logs": "docker-compose logs -f postgres",
    "db:migrate": "node dist/migrate.js"
  },
  "keywords": [
    "qbuilder",
    "example"
  ],
  "author": "Ali Reza Yahya",
  "license": "MIT",
  "dependencies": {
    "qbuilder": "file:..",
    "express": "^5.1.0",
    "pg": "^8.13.1"
  },
  "devDependencies": {
    "@types/express": "^5.0.5",
    "@types/node": "^24.10.1",
    "@types/pg": "^8.15.6",
    "typescript": "^5.9.3"
  }
}
